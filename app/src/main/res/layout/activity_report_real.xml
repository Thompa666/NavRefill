<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.kuduta.navrefill.ReportRealActivity">

    <TextView
        android:id="@+id/tvRepText"
        android:layout_width="318dp"
        android:layout_height="378dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="37dp"
        android:layout_marginLeft="33dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btnRepExit"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="@string/app_main_home"
        app:layout_constraintLeft_toRightOf="@+id/btnRepReload"
        android:layout_marginLeft="-8dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/tvRepText" />

    <Button
        android:id="@+id/btnRepReload"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="@string/app_reload"
        tools:layout_editor_absoluteX="16dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/tvRepText" />
</android.support.constraint.ConstraintLayout>
